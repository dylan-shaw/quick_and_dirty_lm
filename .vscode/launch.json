    "version": "0.2.0",
    "configurations": [
        {
            "name": "tests/pretrain",
            "type": "debugpy",
            "request": "launch",
            "module": "tests.pretrain",
            "args": [
                "pretrain"
            ]
        },
        {
            "name": "examples/gpt/train_tokenizer",
            "type": "debugpy",
            "request": "launch",
            "module": "examples.gpt.train_tokenizer",
            "env": {
                "HF_TOKEN": "${input:hf_token}"
            }
        },
        {
            "name": "examples/gpt/pretrain",
            "type": "debugpy",
            "request": "launch",
            "module": "examples.gpt.pretrain"
        }
    ],
    "inputs": [
        {
            "id": "hf_token",
            "description": "The optional token for downloading content from hugging face",
            "type": "promptString",
            "default": ""
        }
    ]
}